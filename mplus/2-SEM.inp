TITLE:	Confirmatory Factor Analysis;	

DATA:	FILE = mplusdata.csv;

VARIABLE:	NAMES ARE id c1a-c1r f1a-f1g dsnow1-dsnow7 dsfut1-dsfut7 f5a-f5c;
	MISSING ARE ALL (99);
	USEVARIABLES ARE c1j-c1r f1a-f1g dsfut1-dsfut7 f5a-f5c;
	
MODEL:	respon BY c1j-c1n;
	freedom BY c1o-c1r;
	effect BY f1a-f1g;
	risk BY f5a-f5c;
	future BY dsfut1-dsfut7;
		
	effect ON respon freedom;
	risk ON respon freedom;
	future ON effect risk;
	
OUTPUT: 	MODINDICES;
