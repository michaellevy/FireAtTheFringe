TITLE:	Structural Equation Model - Current behavior with all variables;	

DATA:	FILE = mplusdata.csv;

VARIABLE:NAMES ARE id biocen1-biocen4 anthro1-anthro5 
		respons1-respons5 freedom1-freedom4 effect1-effect7 
		dsnow1-dsnow7 dsfut1-dsfut7 risk1-risk3 
		trust1-trust3 trustFS1-trustFS4 exp1-exp3 
		neighb1-neighb7 nearcnf near03f;
	CATEGORICAL ARE 
		!dsnow1-dsnow7 
		exp1-exp3;
	MISSING ARE ALL (99);
	USEVARIABLES ARE 
		biocen1-biocen4 
		anthro1-anthro5 
		respons1-respons5 
		freedom1-freedom4 
		effect1-effect7 
		!dsnow1-dsnow7 
		dsfut1-dsfut7 
		risk1-risk3 
		trust1-trust3 
		trustFS1-trustFS4 
		exp1-exp3 
		neighb1-neighb7;
	
MODEL:	biocen BY biocen1-biocen4;
	anthro BY anthro1-anthro5;
	respons BY respons1-respons5;
	freedom BY freedom1-freedom4;
	effect BY effect1-effect7;
	!current BY dsnow1-dsnow7;
	future BY dsfut1-dsfut7;
	risk BY risk1-risk3;
	trust BY trust1-trust3 trustFS1-trustFS4;
	experien BY exp1-exp3;
	norm BY neighb1-neighb7;
		
	effect ON respons freedom;
	risk ON respons freedom;
	trust ON respons freedom;
	!current ON effect risk trust experien norm;
	future ON effect risk trust experien norm;
		
OUTPUT: 	MODINDICES;
